   "source": [
    "import numpy as np\n",
    "arr1 = np.arange(1, 11)\n",
    "print(\"1D Array:\", arr1)\n",
    "\n",
    "arr2 = np.random.randint(1, 20, size=(3, 3))\n",
    "print(\"2D Array:\\n\", arr2)\n",
    "\n",
    "print(\"Sum:\", arr1.sum())\n",
    "print(\"Mean:\", arr1.mean())\n",
    "print(\"Max:\", arr1.max())\n",
    "\n",
    "arr_multiplied = arr1 * 2\n",
    "print(\"Multiplied by 2:\", arr_multiplied)\n",
    "\n"
    "arr_replaced = arr1.copy()\n",
    "arr_replaced[arr_replaced > 5] = 0\n",
    "print(\"Values > 5 replaced with 0:\", arr_replaced)"
   ]
